    <ul class="nav navbar-nav">
      <li class="nav-item active">
        <%= link_to "Timeline", photos_path, class: 'nav-link' %>
      </li>
    </ul>
  </nav>
  <div class="container">
    <%= yield %>
	</div>
  </body>
</html>
